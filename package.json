{
    "name": "confettibot",
    "version": "1.0.0",
    "description": "README.md",
    "main": "app.js",
    "author": "FG",
    "engines": {
        "node": "10.15.3"
    },
    "scripts": {
        "dev:client": "npm start --prefix client/",
        "dev:server": "npm run dev --prefix server/",
        "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
        "build": "npm run build --prefix client/ && npm run build --prefix server/ && npm install --prefix build/",
        "start": "cd build/ && pm2 start app.js",
        "stop": "pm2 stop all",
        "delete": "pm2 delete app",
        "reload": "npm run stop && npm run delete && npm run build && npm run start"
    },
    "dependencies": {
        "concurrently": "^4.1.0",
        "memorystore": "^1.6.1"
    }
}